@model KeyValuePair<bool, int[]>
    @{
       ViewBag.Title = "Index";
    }

    <h2>@Model.Key.ToString()</h2>
    @if (Model.Key)
    {
        for(int i = 0; i < Model.Value.Length; i++)
        {
            <div class="form-group">
                Value @((i+1).ToString())
                <div>
                    @Model.Value[i].ToString()
                </div>
            </div>
            
        }
    }

